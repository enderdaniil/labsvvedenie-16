#include <iostream>
using namespace std;
int main()
{
    int n,k;
    k=0;
    cout<<"Vvedite kolvo elementov\n";
    cin>>n;
    cout<<"Vvedite massiv\n";
    int* a = new int[n];
    cin>>a[0];
    for (int i=0; i<n; i++)
    {
        cin>>a[i];
        if (*a[i]==*a[i-1])
        {
            k++;
        }
    }
    int b[n-k];
    for (int i=0; i<n-1; i++)
    {
        if (*a[i]==*a[i+1])
        {
            a[i]=0;
        }
    int t=0;
    for (int i=0; i<n; i++)
    {
        if (*a[i]!=0)
        {
            b[t]=a[i];
            t++;
        }
    }
    delete[] a;
    int* a = new int[n-k];
    for (int i=0; i<n-k; i++)
    {
        a[i]=b[i];
    }
    for (int i=0; i<n-k; i++)
    {
        cout<<*a[i]<<endl;
    }
    return 0;
}
